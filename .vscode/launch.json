{
    "configurations": [
        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rs-client'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rs-client",
                    "--package=rs-client"
                ],
                "filter": {
                    "name": "rs-client",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "LD_LIBRARY_PATH": "${workspaceFolder}/target/debug/deps:${workspaceFolder}/target/debug/deps:${userHome}/.rustup/toolchains/stable-x86_64-unknown-linux-gnu/lib/rustlib/x86_64-unknown-linux-gnu/lib"
     }
        }
    ]
}